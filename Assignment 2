 
 ​n=int(input('enter n'))  
 ​l=[]  
 ​for i in range(n):  
 ​    x=int(input())  
 ​    l.append(x)  
 ​print(l)  
 ​t=[]  
 ​for i in l:  
 ​    t.append(int(i))  
 ​print(t)  
 ​N=[]  
 ​for i in t:  
 ​    if i not in N:  
 ​        N.append(i)  
 ​print(N)  
 ​N.sort(reverse=True)  
 ​print(N)  
 ​print(N[1])  
  
  
  
  
  
